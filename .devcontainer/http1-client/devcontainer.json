{
    "name": "connect-http-method-playground-http1-client",
    "dockerComposeFile": ["../compose.yaml", "./compose.yaml"],
    "service": "http1-client",
    "workspaceFolder": "/workspace/http1-client",
    "mounts": [
        "source=${env:HOME}/.claude,target=/root/.claude,type=bind"
    ],
    "customizations": {
        "vscode": {
            "extensions": [
                "golang.Go",
                "GitHub.copilot",
                "RooVeterinaryInc.roo-cline",
                "bufbuild.vscode-buf"
            ]
        }
    },
    "remoteUser": "root"
}
